{
  "name": "Vipassana",
  "dark": true,
  "author": "Marius Ileana",
  "editorScheme": "/META-INF/intellij-vipassana.xml",
  "colors": {
    "defBg": "#324649",
    "defLighterBg": "#3C5458",
    "defSepColor": "#263033",
    "freshG": "#6BE320",
    "lightBlue": "#83B3BB",
    "darkerBlue": "#445B5F"
  },
  "ui": {
    "*": {
      "background": "defBg",
      "caretForeground": "freshG",
      "separatorColor": "defBg",
      "selectionBackground": "defSepColor",
      "selectionForeground": "freshG",
      "Header": {
        "background": "defBg"
      }
    },
    "ActionButton": {
      "hoverBackground": "darkerBlue",
      "hoverBorderColor": "darkerBlue",
      "pressedBorderColor": "freshG",
      "pressedBackground": "defSepColor"
    },
    "Borders": {
      "color": "defBg",
      "ContrastBorderColor": "defBg"
    },
    "Button": {
      "foreground": "lightBlue",
      "background": "defBg",
      "default.startBackground": "defBg",
      "default.endBackground": "defBg",
      "default.focusColor": "freshG",
      "default.foreground": "freshG",
      "startBackground": "defBg",
      "endBackground": "defBg"
    },
    "Component": {
      "infoForeground": "lightBlue",
      "borderColor": "darkerBlue",
      "focusColor": "freshG",
      "focusWidth": 1,
      "hoverIconColor": "freshG"
    },
    "FormattedTextField":{
      "background": "defBg",
      "selectionBackground": "defSepColor"
    },
    "DefaultTabs": {
      "background": "defBg",
      "borderColor": "defBg",
      "underlinedTabBackground": "defBg",
      "underlinedTabForeground": "freshG",
      "inactiveUnderlineColor": "defBg",
      "hoverBackground": "defBg",
      "underlineHeight": 0,
      "underlineColor": "defBg"
    },
    "Editor": {
      "foreground": "#CCC"
    },
    "EditorTabs": {
      "borderColor": "defBg",
      "underlineColor": "freshG",
      "underlineHeight": 0,
      "underlinedTabBackground": "defBg"
    },
    "NavBar.borderColor": "defBg",
    "Separator": {
      "separatorColor": "defbg"
    },
    "ToolBar": {
      "shadow": "defBg"
    },
    "ToolWindow": {
      "Header": {
        "background": "defBg",
        "borderColor": "defBg",
        "inactiveBackground": "defBg"
      },
      "Button.hoverBackground": "darkerBlue"

    },
    "Tree": {
      "background": "defLighterBg",
      "foreground": "#CCC",
      "paintLines": false,
      "selectionBackground": "defBg",
      "selectionForeground": "#8BE04C",
      "selectionInactiveBackground": "#3C5458"
    },
    "Viewport": {
      "background": "#defBg"
    },
    "WelcomeScreen": {
      "background": "defBg",
      "separatorColor": "defSepColor",
      "Projects.background": "defLighterBg"
    }
  }
}
